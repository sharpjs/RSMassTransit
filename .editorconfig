# Documentation:
# https://editorconfig.org
# https://aka.ms/editorconfigdocs

root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.{json,jsonc,md,xml,yaml,yml}]
indent_size = 2

[*.{sln,*proj,props,targets}]
charset = utf-8-bom
indent_size = 2
